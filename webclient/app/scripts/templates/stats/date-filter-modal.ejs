<form class="form-horizontal">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">From</label>
        <div class="col-sm-10">
            <input type="text" class="form-control calendar" name="from" value="<%- moment().subtract(1, "days").format("YYYY-MM-DD HH:mm:ss") %>">
        </div>
    </div>
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">To</label>
        <div class="col-sm-10">
            <input type="text" class="form-control calendar" name="to" value="<%- moment().format("YYYY-MM-DD HH:mm:ss") %>">
        </div>
    </div>
</form>
